    "object-hash": "^3.0.0",
    "prop-types": "^15.8.1",
    "react-immutable-proptypes": "^2.2.0",
    "react-markdown": "^9.0.0",
    "react-popper": "^2.3.0",
    "react-redux": "^8.1.3",
    "react-slick": "^0.29.0",
    "react-textarea-autosize": "^8.5.3",
    "redux": "^4.2.1",
    "slick-carousel": "^1.8.1",
    "socket.io": "^4.7.2",
